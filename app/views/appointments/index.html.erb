<h1>Upcoming Appointments</h1>
<table>
  <tr>
    <th>Start</th>
    <th>Summary</th>
  </tr>
  <% @events.each do |event| %>
    <% if event['start']['dateTime'] %>
      <tr>
        <td><%= event['start']['dateTime'] %></td>
        <td><%= event['summary']%></td>
      </tr>
    <% end %>
  <% end %>
</table>
